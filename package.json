{
  "name": "image-gallery-server",
  "version": "1.0.0",
  "description": "Node server to use with React app image-gallery",
  "main": "server.js",
  "scripts": {
    "test:watch:nodemon": "nodemon --watch lib --watch tests --exec 'npm test'",
    "test:watch": "npm test -- -w",
    "test": "mocha --recursive './src/tests' --reporter mocha-better-spec-reporter",
    "start": "npm run test:watch:nodemon"
  },
