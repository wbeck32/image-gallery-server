  "devDependencies": {},
  "dependencies": {
    "body-parser": "^1.17.2",
    "chai": "^4.1.1",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "mocha": "^3.5.0",
    "mocha-better-spec-reporter": "^3.1.0",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.9",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "superagent": "^3.6.0"
